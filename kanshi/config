output 'BOE 0x0BCA Unknown' {
  alias $LAPTOP
  mode 2256x1504@60.000
  scale 1.6
}

output 'ViewSonic Corporation VA1655-FHD WMM224540834' {
  alias $PORTABLE
  mode 1920x1080@60.000
  scale 1.2
}


profile laptop-and-portable {
  output $LAPTOP position 0,0
  output $PORTABLE position 1412,0
}

profile laptop-only {
  output $LAPTOP position 0,0
}
